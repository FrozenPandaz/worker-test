{
  "version": 1,
  "projects": {
    "worker-test": {
      "root": "apps/worker-test",
      "sourceRoot": "apps/worker-test/src",
      "projectType": "application",
      "prefix": "worker-test",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/worker-test",
            "main": "apps/worker-test/src/main.ts",
            "tsConfig": "apps/worker-test/tsconfig.app.json",
            "assets": [
              "apps/worker-test/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/worker-test/src/environments/environment.ts",
                  "with": "apps/worker-test/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "buildTarget": ""
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/worker-test/tsconfig.app.json",
              "apps/worker-test/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/worker-test/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/worker-test/jest.config.js",
            "tsConfig": "apps/worker-test/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "worker-test"
}
